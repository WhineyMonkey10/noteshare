{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb"
            }

            
            
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ],

    "projectSettings": {
        "framework": "python",
        "devCommand": "python main.py",
        "buildCommand": "python main.py",
        "outputDirectory": "public"
    }
  }
  